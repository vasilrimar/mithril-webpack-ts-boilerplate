{
  "extends": [
    "tslint:latest"
  ],
  "rules": {
    "arrow-parens": false,
    "ban-types": [
      false
    ],
    "curly": false,
    "indent": [
      false
    ],
    "max-classes-per-file": false,
    "max-line-length": [
      true,
      250
    ],
    "member-access": false,
    "member-ordering": false,
    "no-angle-bracket-type-assertion": false,
    "no-console": [
      false
    ],
    "no-invalid-this": [
      true,
      "check-function-in-method"
    ],
    "no-implicit-dependencies": false,
    "no-object-literal-type-assertion": false,
    "no-submodule-imports": false,
    "no-this-assignment": false,
    "object-literal-key-quotes": [
      true,
      "as-needed"
    ],
    "object-literal-sort-keys": false,
    "ordered-imports": [
      false
    ],
    "prefer-for-of": false,
    "quotemark": [
      true,
      "avoid-escape",
      "single"
    ],
    "radix": false,
    "trailing-comma": [
      false
    ],
    "variable-name": [
      true,
      "allow-leading-underscore",
      "allow-pascal-case",
      "ban-keywords",
      "check-format"
    ]
  }
}
