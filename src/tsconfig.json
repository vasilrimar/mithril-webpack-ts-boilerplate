{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "importHelpers": true,
    "lib": [
      "dom",
      "es7"
    ],
    "module": "commonjs",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "stripInternal": true,
    "target": "es5",
    "typeRoots": [
      "./node_modules/@types"
    ]
  }
}
